		{
			"name": "Grab On",
			"type": "maneuver",
			"img": "icons/skills/melee/hand-grip-staff-teal.webp",
			"effects": [],
			"flags": {
				"core": {
					"sourceId": "Compendium.a5e.a5e-maneuvers.jewmp4pzrg9cdui9"
				}
			},
			"system": {
				"ability": "",
				"actionOptions": [
					"savingThrow"
				],
				"activation": {
					"cost": 1,
					"type": "action",
