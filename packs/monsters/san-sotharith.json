		{
			"_id": "GK3sFFa23WXsmaGR",
			"name": "Possession (Recharge 6)",
			"type": "feature",
			"img": "icons/svg/item-bag.svg",
			"effects": [],
			"folder": null,
			"sort": 600000,
			"flags": {
