{"folder":null,"name":"Lightless Perception","type":"feature","img":"icons/magic/light/explosion-star-small-blue-yellow.webp","system":{"ac":{"baseFormula":"","formula":"","grantsDisadvantage":false,"maxDex":0,"minStr":0,"mode":2,"requiresNoShield":false,"requiresUnarmored":false},"actions":{},"description":"<p>If you do not have darkvision, you gain darkvision with a range of 60 feet. If you already have darkvision, its range increases to 120 feet.</p>","favorite":false,"secretDescription":"","source":"dungeonDelversGuide","uses":{"value":0,"max":"","per":"","recharge":{"formula":"1d6","threshold":6}},"concentration":false,"featureType":"culture","grants":{},"prerequisite":"","requiresBloodied":false,"schemaVersion":{"version":0.019,"lastMigration":{"version":{"schema":0.018,"foundry":"11.315","system":"0.18.11"}}},"classes":""},"effects":[{"icon":"icons/svg/item-bag.svg","origin":"Item.f8fg68oxiwplumqx","flags":{"a5e":{"sort":0,"transferType":"passive"}},"transfer":false,"name":"Darkvision","_id":"4akpI2pAzgBEI9X6","changes":[{"key":"system.attributes.senses.darkvision.distance","value":"60","mode":2,"priority":null}],"disabled":false,"duration":{"startTime":null,"seconds":null,"combat":null,"rounds":null,"turns":null,"startRound":null,"startTurn":null},"description":"","statuses":[],"folder":null}],"flags":{},"_id":"f8fg68oxiwplumqx"}
