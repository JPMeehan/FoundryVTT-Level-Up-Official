mers":{"CvSjrqMHfmLZbrBx":{"type":"itemUses","default":true,"quantity":15},"iXJF8sMZDypfRjTO":{"type":"ammunition","default":true,"itemId":"","quantity":1}},"rolls":{"fiDRLl2X777LtwXS":{"type":"damage","default":true,"formula":"2d10","damageType":"piercing","canCrit":false}},"prompts":{"BMVzy4H8FjwoZ7cB":{"type":"savingThrow","default":true,"ability":"dex","saveDC":{"type":"custom","bonus":"8 + @prof + @int.mod"},"onSave":"Half damage"}},"uses":{"value":0,"max":"","per":"","recharge":{"formula":"1d6","rechargeType":"custom","rechargeAmount":"1","threshold":6}},"duration":{"unit":"turn","value":1},"area":{"shape":"cone","quantity":1,"length":90,"placeTemplate":true}}},"description":"<p>&nbsp;This functions as a heavy arbalest, except it may preload up to 15 shots at a time. &nbsp;Loading it takes an action, and while so loaded it may fire as if it did not have the loading property. &nbsp;Alternatively, you may expend all preloaded bolts in one round as an action. &nbsp;If you do so, you create a 90 ft cone. &nbsp;Creatures that enter that cone or start their turn in it must make a dexterity saving throw, taking 2d10 on a failed save or half on a success, and the area of the cone is difficult terrain until the start of your next turn.</p>","favorite":false,"secretDescription":"","source":"extraCreditAndDeeperMysteries","uses":{"value":0,"max":"15","per":"","recharge":{"formula":"1d6","threshold":6}},"ammunitionProperties":[],"armorCategory":"","armorProperties":[],"attuned":false,"bulky":true,"craftingComponents":"","containerId":"","damagedState":0,"equippedState":0,"items":{},"materialProperties":[],"objectType":"weapon","plotItem":false,"price":"0","proficient":true,"quantity":1,"rarity":"mundane","requiresAttunement":false,"shieldCategory":"","shieldProperties":[],"unidentified":false,"unidentifiedDescription":"","unidentifiedName":"","weaponProperties":["loading","range","twoHanded"],"weight":100,"schemaVersion":{"version":0.018,"lastMigration":null},"breakerProp